# Redwood Config
CONFIG_MACH_XIAOMI_REDWOOD=y

# Charger Configs
CONFIG_BQ_FUEL_GAUGE=y
CONFIG_BQ_CLOUD_AUTHENTICATION=y
CONFIG_BQ_FG_1S=y
CONFIG_BQ2597X_BATTERY_CHARGER=y
CONFIG_AI_RSOC=y

# File systems
CONFIG_EXFAT_FS=y
CONFIG_EXFAT_DEFAULT_IOCHARSET="utf8"
CONFIG_NTFS_FS=y
# CONFIG_NTFS_DEBUG is not set
# CONFIG_NTFS_RW is not set

# Finger print Configs
CONFIG_FINGERPRINT_GOODIX_TEE=m
CONFIG_FINGERPRINT_XIAOMI_FP=m

# Haptics Config
CONFIG_INPUT_AW86927_HAPTIC=m

# KernelSU
CONFIG_KSU=y

# Touch Panel Configs
CONFIG_TOUCHSCREEN_GOODIX_BRL=m
CONFIG_TOUCHSCREEN_GOODIX_BRL_SPI=y
CONFIG_TOUCHSCREEN_GOODIX_BRL_QGKI=y

# Disable mmc driver
# CONFIG_MMC is not set

# Disable fsa4480 (Redwood doesn't support usb audio)
# CONFIG_QCOM_FSA4480_I2C is not set
